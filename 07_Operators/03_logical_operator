# LOGICAL OPERATORS
c = True
d = False

print(c and c)
print(c and d)
print(d and c)
print(d and d)
# 'and' always returns a true if both the values are true otherwise if a single value is false then it will always show the result false as in above example

print(True and True)
print(True and False)
print(False and True)
print(False and False)

print(True or True)
print(True or False)
print(False or True) # 'or' will always return true if single value is true as observed in this given example
print(False or False)

print('Not Operator')
print(not(True))
print(not(False))